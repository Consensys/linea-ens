{
  "name": "@ensdomains/ens-contracts",
  "version": "1.0.0",
  "description": "Linea ENS contracts",
  "homepage": "https://github.com/ensdomains/ens-contracts#readme",
  "bugs": {
    "url": "https://github.com/ensdomains/ens-contracts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ensdomains/ens-contracts.git"
  },
  "license": "(MIT OR Apache-2.0)",
  "author": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "build",
    "contracts/**/*.sol",
    "artifacts",
    "deployments/mainnet/*.json",
    "deployments/archive/**/*.json"
  ],
  "scripts": {
    "build": "rm -rf ./build/deploy ./build/hardhat.config.js && hardhat compile && tsc",
    "compile": "hardhat compile",
    "format": "prettier --write .",
    "lint": "hardhat check",
    "prepublishOnly": "yarn build",
    "test": "hardhat test",
    "test:deploy": "hardhat --network hardhat deploy",
    "test:local": "hardhat --network localhost test"
  },
  "resolutions": {
    "ethereum-ens": "0.8.0",
    "js-sha3": "0.8.0"
  },
  "dependencies": {
    "@ensdomains/buffer": "^0.1.1",
    "@ensdomains/solsha1": "0.0.3",
    "@openzeppelin/contracts": "^5.1.0",
    "axios": "^1.8.4",
    "csv-parse": "^5.5.6",
    "dns-packet": "^5.3.0",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@ensdomains/dnsprovejs": "^0.3.7",
    "@ensdomains/test-utils": "^1.3.0",
    "@lavamoat/allow-scripts": "^3.4.0",
    "@lavamoat/preinstall-always-fail": "^2.1.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "elliptic-solidity": "^1.0.0",
    "envfile": "^6.17.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.6.1",
    "hardhat": "^2.9.9",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.10",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-storage-layout": "^0.1.7",
    "husky": "^8.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "rfc4648": "^1.5.0",
    "solidity-coverage": "^0.8.12",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "volta": {
    "node": "16.20.2"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@nomiclabs/hardhat-truffle5>@nomiclabs/truffle-contract>ethereum-ens>web3": false,
      "@nomiclabs/hardhat-truffle5>@nomiclabs/truffle-contract>ethereum-ens>web3>web3-bzz": false,
      "@nomiclabs/hardhat-truffle5>@nomiclabs/truffle-contract>ethereum-ens>web3>web3-shh": false,
      "@nomiclabs/hardhat-web3>web3": false,
      "@nomiclabs/hardhat-web3>web3>web3-bzz": false,
      "@nomiclabs/hardhat-web3>web3>web3-shh": false,
      "@openzeppelin/test-helpers>web3": false,
      "@openzeppelin/test-helpers>web3>web3-bzz": false,
      "@openzeppelin/test-helpers>web3>web3-shh": false,
      "ethereum-waffle>@ethereum-waffle/provider>ganache-core>ethereumjs-vm>core-js-pure": false,
      "ethereum-waffle>@ethereum-waffle/provider>ganache-core>keccak": false,
      "ethereum-waffle>@ethereum-waffle/provider>ganache-core>web3": false,
      "ethereum-waffle>@ethereum-waffle/provider>ganache-core>web3-provider-engine>eth-block-tracker>json-rpc-engine>babelify>babel-core>babel-runtime>core-js": false,
      "ethereum-waffle>@ethereum-waffle/provider>ganache-core>websocket>es5-ext": false,
      "ethereum-waffle>@ethereum-waffle/provider>postinstall-postinstall": false,
      "hardhat>@nomicfoundation/ethereumjs-blockchain>level>classic-level": false,
      "hardhat>@nomicfoundation/ethereumjs-tx>ethereum-cryptography>secp256k1": false,
      "hardhat>keccak": false,
      "hardhat>ws>bufferutil": false,
      "hardhat>ws>utf-8-validate": false
    }
  }
}
